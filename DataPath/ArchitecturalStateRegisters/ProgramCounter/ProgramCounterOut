    .scope S_0x144608cb0;
T_0 ;
    %wait E_0x144607860;
    %load/vec4 v0x14461f090_0;
    %flag_set/vec4 8;
    %jmp/0 T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %jmp/1 T_0.1, 8;
T_0.0 ; End of true expr.
    %load/vec4 v0x14461ef60_0;
    %jmp/0 T_0.1, 8;
 ; End of false expr.
    %blend;
T_0.1;
    %store/vec4 v0x14461f120_0, 0, 32;
    %jmp T_0;
    .thread T_0;
    .scope S_0x14460d840;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x14461f220_0;
    %inv;
    %store/vec4 v0x14461f220_0, 0, 1;
    %jmp T_1;
